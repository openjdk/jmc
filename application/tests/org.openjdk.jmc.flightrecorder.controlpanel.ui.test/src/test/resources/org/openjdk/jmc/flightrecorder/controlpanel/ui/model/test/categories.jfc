<?xml version="1.0" encoding="UTF-8"?>

<configuration version="2.0" label="For Test" description="" provider="Oracle">
	<category label="Our Special Java Category">
		<category label="Thread">
		    <event name="jdk.ThreadAllocationStatistics" label="Thread Allocation Statistics">
		      <setting name="enabled">true</setting>
		      <setting name="period">everyChunk</setting>
		    </event>
		</category>
	       <event name="jdk.JavaMonitorWait" label="Monitor Wait" prototype="timed">
      <setting name="enabled">true</setting>
      <setting name="stackTrace">true</setting>
      <setting name="threshold" control="synchronization-threshold">27 ms</setting>
    </event>
	</category>
	<!-- Uncategorized-->
	<event name="jdk.ActiveRecording" label="Active Recording" prototype="instant">
      <setting name="enabled">true</setting>
    </event>
</configuration>
